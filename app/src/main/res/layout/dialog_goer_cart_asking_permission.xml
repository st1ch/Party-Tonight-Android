<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@color/colorProgressDialog"
    android:orientation="vertical"
    android:padding="8dp">

    <app.media.opp.partytonight.presentation.app.view.TextViewAgudaBold
        android:layout_width="wrap_content"
        android:layout_height="64dp"
        android:layout_gravity="center"
        android:text="@string/goer_cart_permission"
        android:textAlignment="center"
        android:textColor="@android:color/white" />

    <app.media.opp.partytonight.presentation.app.view.ButtonTransparent
        android:id="@+id/bProceed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@null"
        android:text="Proceed"
        android:textColor="@android:color/white"
        android:textStyle="bold" />
</LinearLayout>